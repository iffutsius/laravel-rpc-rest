{
    "name": "iffutsius/laravel-rpc-rest",
    "description": "RPC library to quick start making RPC REST calls to 3rd party APIs",
    "keywords": ["laravel", "rpc", "api"],
    "homepage": "https://github.com/iffutsius/laravel-rpc",
    "license": "MIT",
    "authors": [
      {
        "name": "Ivo Hindreus",
        "email": "iffutsius@gmail.com",
        "homepage": "https://github.com/iffutsius",
        "role": "Creator"
      }
    ],
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
        "laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
      "psr-4": {
        "Iffutsius\\LaravelRpc\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
            "Iffutsius\\LaravelRpc\\RpcServiceProvider"
        ]
      }
    }
}
